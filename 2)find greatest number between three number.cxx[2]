#include<iostream>
using namespace std;
int main()
{
    int a,b,c;
    cin>>a>>b>>c;

    if(a>b)
    {   if(a>c)
        {   cout << "A is larger than other" << endl;
        }
        else
        {
            cout << "C is larger than other" << endl;
        }
    }
    else {
        if(b>c)
        {
            cout << "B is larger than other" << endl;
        }
        else
        {
            cout << "C is larger than other" << endl;
        }
    }
    return 0;
}